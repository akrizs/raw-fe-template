{
  "name": "raw-fe-template",
  "version": "0.5.0",
  "description": "A base template for simple frontend beginning!",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=development webpack-dev-server --config webpack.config.js --inline --progress --colors",
    "dev": "env NODE_ENV=development webpack --config webpack.config.js",
    "watch": "env NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "env NODE_ENV=production webpack --config webpack.config.js"
  },
  "keywords": [
    "template",
    "frontend",
    "starter"
  ],
  "author": "Anton Kristensen <anton@akr.is>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@littlemissrobot/sass-breakpoints": "^1.0.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^9.0.2",
    "scss-resets": "^0.5.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
